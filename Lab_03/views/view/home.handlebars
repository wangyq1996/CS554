<section>
    <ul class="full-list" id="listall">
        {{#each allData}}
            <li class="li-item">
                <a href="/show/{{this.id}}" title="movie-{{this.name}}">
                    {{this.name}}
                </a>
            </li>
        {{/each}}
    </ul>
    {{#if token}} 
        <a href="/">Go Back!</a> 
    {{/if}}
</section>